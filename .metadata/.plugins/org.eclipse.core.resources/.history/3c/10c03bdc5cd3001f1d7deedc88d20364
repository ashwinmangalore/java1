package pkg2;

import java.util.ArrayList;
import java.util.List;

public class A12 {

	public static void main(String[] args) {
		String str1="ABC";
		String s1;
		String s2;
		String s3;
		s1=str1.substring(0, 1);
		s2=str1.substring(1, 2);
		s3=str1.substring(2, 3);
		String arr[]= {};
		arr[0]=s1;
		arr[1]=s2;
		arr[2]=s3;
		arr[3]=s1.concat(s2);
		arr[4]=s2.concat(s3);
		arr[5]=s3.concat(s1);
		arr[6]=arr[3].concat(s3);
		for(int i=0;i<7;i++) {
			System.out.print(arr[i]);
		}
		

		
		
		
	}

}
