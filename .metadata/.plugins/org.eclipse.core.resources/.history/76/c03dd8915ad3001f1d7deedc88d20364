package pkg2;

import java.util.ArrayList;
import java.util.List;

public class A12 {

	public static void main(String[] args) {
		String str1="ABCA";
		String s1;
		List<String>list1=new ArrayList<>();
		for(int j=0;j<2;j++) {
		for(int i=1;i<=3;i++){
	    s1=str1.substring(i-1, i+j);
		list1.add(s1);
		}
		}
		System.out.print(list1);
	}

}
