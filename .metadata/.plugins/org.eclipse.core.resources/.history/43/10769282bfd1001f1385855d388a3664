package pkg2;

import java.util.Arrays;

public class A9 {

	public static void main(String[] args) {
		int arr1[]= {1,2,3,-1,-2,-3};
		//1,-1,2,-2,3,-3
		//arr1[o]  0<=i<=length
		//compare arr1[0]+arr1[i]=0
		int temp1=0;
		Arrays.sort(arr1);
		for(int i=0;i<6;i++) {
		if(arr1[0]+arr1[i]==0) {
			System.out.print(arr1[0]+","+arr1[i]);
		}
		if(arr1[1]+arr1[i]==0) {
			System.out.print(arr1[1]+","+arr1[i]);
		}
		if(arr1[2]+arr1[i]==0) {
			System.out.print(arr1[2]+","+arr1[i]);
		}
		}
	}

}
