package pkg1;
public class A18 {
	public static void reverse(String word1) {
		String str1=word1;
		String str2="";
		int len1=str1.length();
		for(int i=1;i<len1+1;i++) {
			str2=str1.substring(len1-i,(len1+1)-i);
			System.out.print(str2);		
		}	
		
	}
	public static void main(String[] args) {
		reverse("Ashwinmangalore");
		reverse("Chandrashekar Rao Kuthyar");
		reverse("Pranatharthiharan");
		reverse("Prakhyath Shetty");
		
	}
}
