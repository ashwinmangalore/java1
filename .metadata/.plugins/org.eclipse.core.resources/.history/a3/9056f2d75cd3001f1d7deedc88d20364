package pkg2;

import java.util.ArrayList;
import java.util.List;

public class A12 {

	public static void main(String[] args) {
		String str1="ABC";
		Character s1;
		List<Character>list1=new ArrayList<>();
		for(int i=0;i<3;i++){
	    s1=str1.charAt(i);
		list1.add(s1);
		}
		
		System.out.print(list1);
	}

}
