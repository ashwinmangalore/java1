package pkg2;

import java.util.Arrays;

public class A10 {
	public static void number(int arr1[]) {
		
	}
	public static void Position(int arr1[]) {
		int len1;
		int pos;
		len1=arr1.length;
		Arrays.sort(arr1);
		System.out.println(arr1[0]);
		for(int i=0;i<len1;i++) {
			for(int j=0;j<len1;j++) {
				while(i!=j) {
				if(Math.abs(arr1[i])==arr1[j]){
					pos=j;
				}
			}
		}	
	}
	}

	public static void main(String[] args) {
		int arr1[]= {1,-1,2,-2,3,-3};
        Arrays.sort(arr1);
	}

}
