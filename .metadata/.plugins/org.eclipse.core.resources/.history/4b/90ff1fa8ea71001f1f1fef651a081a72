package pkg1;
public class A18 {
	
	public static void main(String[] args) {
		String str1="AshwinMangalore";
		String str2="";
		int len1=str1.length();
		for(int i=1;i<len1+1;i++) {
			str2=str1.substring(len1-i,(len1+1)-i);
			System.out.print(str2);		
		}
		
		String str1="ShrinathM";
		String str2="";
		int len2=str1.length();
		for(int i=1;i<len2+1;i++) {
			str2=str1.substring(len2-i,(len2+1)-i);
			System.out.print(str2);		
		}	
	}
}
